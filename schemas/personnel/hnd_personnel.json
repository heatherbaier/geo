{
    "$schema": "https://specs.frictionlessdata.io/table-schema.json",
    "name": "hnd_personnel",
    "title": "Honduras — Personnel Table",
    "description": "Teacher and student count data by school and year, disaggregated by gender.",
    "version": "1.0.0",
    "licenses": [
      {
        "name": "CC‐BY‐4.0",
        "title": "Creative Commons Attribution 4.0",
        "path": "https://creativecommons.org/licenses/by/4.0/"
      }
    ],
    "sources": [
      {
        "title": "Honduras Ministry of Education",
        "path": "http://estadisticas.se.gob.hn/see/archivos_descargables.php"
      }
    ],
    "homepage": "https://github.com/Global-Education-Observatory/geo-dataset/blob/master/files_for_db/personnel/hnd_personnel.csv",
    "created": "2025-04-29",
    "modified": "2025-04-29",
    "primaryKey": ["geo_id", "year"],
    "fields": [
      { "name": "geo_id", "type": "string", "description": "Unique GEO school identifier" },
      { "name": "deped_id", "type": "string", "description": "Official Ministry of Education school code" },
      { "name": "year", "type": "integer", "description": "Year of record" },
      { "name": "total_teacher_male", "type": "integer", "description": "Number of male teachers" },
      { "name": "total_teacher_female", "type": "integer", "description": "Number of female teachers" },
      { "name": "total_teachers", "type": "integer", "description": "Total number of teachers" },
      { "name": "total_student_male", "type": "integer", "description": "Number of male students" },
      { "name": "total_student_female", "type": "integer", "description": "Number of female students" },
      { "name": "total_student_enrollment", "type": "integer", "description": "Total student enrollment" }
    ]
  }
  